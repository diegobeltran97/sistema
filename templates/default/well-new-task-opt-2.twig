{% if prefix == 's1_' %}
    {% set bomba = extraccion_bomba %}
    {% set motor = extraccion_motor %}
    {% set acc = extraccion_acc %}
{% elseif prefix == 's2_' %}
    {% set bomba = instalacion_bomba %}
    {% set motor = instalacion_motor %}
    {% set acc = instalacion_acc %}
{% endif %}

{% if attribute(motor, prefix ~ 'tipo_motor') is defined %}
    {% set tipo_motor =  attribute(motor, prefix ~ 'tipo_motor') %}
{% else %}
    {% set tipo_motor = 1 %}
{% endif %}


<div class="form-group">
    <div class="col-sm-4">
        <label class="control-label" for="{{ prefix }}tipo_bomba">Tipo</label>
        <select name="{{ prefix }}tipo_bomba" id="{{ prefix }}tipo_bomba" class="form-control equipment_type  select-default">
            {% for tipo_bomba in tipos_bombas %}
                <option value="{{ tipo_bomba.equip_tipo_id }}" {% if bomba.tipo == tipo_bomba.equip_tipo_id %} selected {% endif %}>{{ tipo_bomba.nombre|raw }}</option>
            {% endfor %}
        </select>
    </div>

</div>

